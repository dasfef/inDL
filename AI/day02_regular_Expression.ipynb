   "source": [
    "# 정규표현식(Regular Expression)\n",
    "import re\n",
    "\n",
    "def solution(new_id) :\n",
    "    #1\n",
    "    new_id = new_id.lower()\n",
    "\n",
    "    #2\n",
    "    re.sub('[^0-9a-z\\-_.]*', \"\", new_id)\n",
    "\n",
    "    #3\n",
    "    re.sub('[.]+', \".\", new_id)\n",
    "\n",
    "    #4\n",
    "    re.sub('^[.][.]$', \"\", new_id)"
   ]
  }
 ],
